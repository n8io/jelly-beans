{
  "name": "@n8io/jelly-beans",
  "main": "build/static/js/main.js",
  "version": "0.1.3",
  "license": "MIT",
  "description": "A React component for generating visual lorem ipsum",
  "homepage": "<%= homepageUrl %>",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build:storybook": "build-storybook -s public",
    "build": "react-app-rewired build --library",
    "clean": "rm -rf node_modules && yarn",
    "commit": "git-cz",
    "coverage": "yarn -s run test -- --coverage",
    "deploy": "gh-pages -d storybook-static",
    "dev": "yarn -s start",
    "predeploy": "yarn -s run build:storybook",
    "prepare": "yarn -s run build",
    "start": "start-storybook -p 9009 -s public",
    "test": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "config": {
    "cz-emoji": {},
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "devDependencies": {
    "@storybook/addon-controls": "^6.0.27",
    "@storybook/addon-docs": "^6.0.27",
    "@storybook/addons": "^6.0.27",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.27",
    "@testing-library/react": "^11.1.0",
    "change-case": "^4.1.1",
    "commitizen": "^4.0.4",
    "cz-emoji": "^1.2.1",
    "eslint-plugin-import": "^2.20.2",
    "gh-pages": "^2.2.0",
    "jest-styled-components": "^7.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "plop": "^2.6.0",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.2",
    "title-case": "^3.0.2",
    "webpack": "^4.42.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "randomcolor": "^0.6.2",
    "styled-components": "^5.2.0"
  }
}
